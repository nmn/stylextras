{
  "name": "@stylextras/stylex-include",
  "version": "0.1.0",
  "description": "A plugin to retroactively support the legacy `stylex.include` API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/nmn/stylextras",
  "license": "MIT",
  "scripts": {
    "build": "find ./src -name '*.ts' -exec sh -c 'bun build {} --outfile ./lib/$(basename {} .ts).js --target node --no-bundle' \\;",
    "test": "vitest"
  },
  "dependencies": {
    "@babel/core": "^7.24.0",
    "@babel/generator": "^7.28.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@types/babel__core": "^7.20.5",
    "vitest": "^1.4.0",
    "webpack": "^5.101.0"
  }
}