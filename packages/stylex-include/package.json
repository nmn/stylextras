{
  "name": "@stylextras/stylex-include",
  "version": "0.2.0",
  "description": "A set of utilities to support usages of the removed `stylex.include` API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./babel-plugin": {
      "require": "./lib/babel-plugin.js",
      "types": "./lib/babel-plugin.d.ts"
    },
    "./webpack-loader": {
      "require": "./lib/webpack-loader.js",
      "types": "./lib/webpack-loader.d.ts"
    }
  },
  "repository": "https://github.com/nmn/stylextras",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf lib && (tsc src/*.ts --outDir lib --declaration --emitDeclarationOnly || exit 0)",
    "build": "bun build ./src/index.ts ./src/babel-plugin.ts ./src/webpack-loader.ts --outdir ./lib --target node --format cjs",
    "test": "bun build ./src/webpack-loader.ts --outfile ./__tests__/tmp/webpack-loader.js --target node --format cjs && vitest"
  },
  "dependencies": {
    "@babel/core": "^7.28.4",
    "@babel/generator": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "babel-loader": "^10.0.0",
    "vitest": "^1.6.1",
    "webpack": "^5.102.0"
  },
  "files": ["lib/*"]
}
